<% @messages.each do |message| %>
<div class="col-md-4">
  <div class="message">
    <h2 class="message_title"><%= message.title %></h2>
    <%= link_to "View Message", message_path(message), class: "btn btn-default" %>
</div>
</div>
<% end %>
